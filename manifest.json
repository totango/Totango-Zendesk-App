{
  "version": "1.2",
  "name": "Totango Insights",
  "author": {
    "name": "Totango",
    "email": "support@totango.com",
    "url": "http://www.totango.com"
  },
  "frameworkVersion": "2.0",
  "location": {
    "support": {
      "ticket_sidebar": "assets/iframe.html",
      "new_ticket_sidebar": "assets/iframe.html",
      "user_sidebar": "assets/iframe.html"
    }
  },
  "defaultLocale": "en",
  "private": false,
  "singleInstall": true,
  "domainWhitelist": ["app.totango.com", "api.totango.com", "api-eu1.totango.com"],
  "parameters": [
    {
      "name": "api_key",
      "type": "password",
      "required": true,
      "secure": true
    },
    {
      "name": "service_id",
      "type": "text",
      "required": false
    },
    {
      "name": "fallback_custom_field",
      "type": "text",
      "required": false
    },
    {
      "name": "fallback_totango_attribute",
      "type": "text",
      "required": false
    },
    {
      "name": "extra_totango_attributes",
      "type": "hidden",
      "required": false
    },
    {
      "name": "totango_attributes",
      "type": "multiline",
      "default": "status\nContract Value\nhealth\nEngagement Score\nUsage Frequency\nAccount Created\nContract Renewal Date\nLicenses\nSuccess Manager\nSales Manager",
      "required": false
    },
    {
      "name": "show_tags",
      "type": "checkbox",
      "required": false,
      "default": true
    },
    {
      "name": "use_hashed_email",
      "type": "checkbox",
      "required": false
    }
  ]
}